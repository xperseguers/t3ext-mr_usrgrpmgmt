{
  "name": "causal/mr_usrgrpmgmt",
  "type": "typo3-cms-extension",
  "description": "This is a Backend-related extension to manage both Backend and Frontend users from the edit form of Backend/Frontend groups. Easily assign users to groups or remove them from groups.",
  "keywords": [
    "TYPO3 CMS",
    "be_groups",
    "be_users",
    "fe_groups",
    "fe_users"
  ],
  "homepage": "https://extensions.typo3.org/extension/mr_usrgrpmgmt",
  "support": {
    "docs": "https://docs.typo3.org/p/causal/mr_usrgrpmgmt/main/en-us/",
    "issues": "https://github.com/xperseguers/t3ext-mr_usrgrpmgmt/issues",
    "source": "https://github.com/xperseguers/t3ext-mr_usrgrpmgmt"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4.0 <=7.4.99 || >=8.0.0 <=8.2.99",
    "typo3/cms-core": "^10 || ^11 || ^12"
  },
  "autoload": {
    "psr-4": {
      "Causal\\MrUsrgrpmgmt\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3.x-dev"
    },
    "typo3/cms": {
      "extension-key": "mr_usrgrpmgmt"
    }
  }
}
